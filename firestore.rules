rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if false;
      allow write: if false;
    }
    
    match /waitingroom/{user} {
      allow read: if request.auth.id != null;
      allow create, delete: if request.auth.uid == user;
    }
  }
}