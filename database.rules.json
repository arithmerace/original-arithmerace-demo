{
  "rules": {
    "ustatus": {
      "$uid": {
        ".write": "$uid === auth.uid",
        ".read": "auth.uid !== null",
        ".validate": "newData.child('status').val() === 'online' || newData.child('status').val() === 'offline'"
      }
    },
    "waitingroom": {
      ".read": "auth.uid !== null",
      "$uid": {
        ".write": "auth.uid === $uid",
      }
    },
    "user": {
      "$uid": {
        ".read": true,
        "profile": {
          ".write": "auth.uid === $uid",
          "username": {
            ".validate": "newData.isString() && newData.val().length <= 16"
          }
        },
        "account": {
          "arithmecoin": {}
        }
      }
    },
    "username": {
      "$uname": {
        ".read": true,
        
      }
    }
    "race": {
      "$raceid": {
        ".read": "auth.uid !== null",
        "player": {
          "$playerid": {
            ".write": false
          }
        }
      }
    },
    "_race": {
      ".read": false
    }
  }
}